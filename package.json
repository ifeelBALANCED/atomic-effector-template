{
  "name": "atomic-effector-template",
  "version": "0.0.0",
  "type": "module",
  "description": "A lightweight template integrating Vite, Effector, and Atomic Router for building modern web applications. This template provides a minimal setup combining Vite's rapid development environment, Effector's state management, and Atomic Router's declarative routing capabilities.",
  "homepage": "",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifeelBALANCED/atomic-effector-template"
  },
  "bugs": {
    "url": "https://github.com/ifeelBALANCED/atomic-effector-template/issues",
    "email": "vladislavherasymenko@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prepare": "husky",
    "ts:check": "tsc --noEmit",
    "fmt": "biome format --write .",
    "lint": "biome lint --error-on-warnings --write .",
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe .",
    "biome:check": "pnpm fmt && pnpm lint && pnpm check",
    "dep-cruiser:preview": "npx depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph-preview.svg"
  },
  "dependencies": {
    "@biomejs/cli-darwin-x64": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@farfetched/core": "^0.13.0",
    "@mantine/core": "^7.15.1",
    "@mantine/dropzone": "^7.15.1",
    "@mantine/hooks": "^7.15.1",
    "@withease/contracts": "^1.1.0",
    "@withease/factories": "^1.0.5",
    "atomic-router": "^0.10.1",
    "atomic-router-react": "^0.10.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "effector": "^23.2.3",
    "effector-react": "^23.3.0",
    "effector-storage": "^7.1.0",
    "history": "^5.3.0",
    "patronum": "^2.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "vite-plugin-checker": "^0.8.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@biomejs/biome": "1.9.4",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "cssnano": "^7.0.6",
    "dependency-cruiser": "^16.8.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.9",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8"
  },
  "author": {
    "name": "ifeelBALANCED",
    "email": "vladislavherasymenko@gmail.com"
  },
  "keywords": [
    "react",
    "pwa",
    "frontend",
    "typescript",
    "vite",
    "effector",
    "mantine",
    "tailwindcss"
  ]
}
